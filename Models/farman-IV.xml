  <!-- Animations des ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronG</object-name>
    <object-name>aileronG2</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry>
        <ind>  1.0 </ind>
        <dep> 15.0 </dep>
      </entry>
      <entry>
        <ind> 0.000 </ind>
        <dep> 0.000 </dep>
      </entry>
      <entry>
        <ind> -1.000 </ind>
        <dep>  0.000 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -0.751 </x-m>
      <y-m> -6.670 </y-m>
      <z-m>  1.289 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation> 

