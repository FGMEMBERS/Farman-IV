    <object-name>cuisseD</object-name>
    <object-name>jambeG</object-name>
    <object-name>jambeD</object-name>
    <object-name>piedG</object-name>
    <object-name>piedD</object-name>

    <!-- Objets transparents -->
    <object-name>profondeur2</object-name>
    <object-name>profondeur4</object-name>
    <object-name>ailes2</object-name>
    <object-name>aileronG2</object-name>
    <object-name>aileronD2</object-name>
    <object-name>directionG2</object-name>
    <object-name>directionD2</object-name>
    <object-name>cables</object-name>
    <object-name>cableprofA</object-name>
    <object-name>cableprof</object-name>
    <object-name>cableprofG1</object-name>
    <object-name>cableprofG2</object-name>
    <object-name>cableprofD1</object-name>
    <object-name>cableprofD2</object-name>
    <object-name>cabledirG</object-name>
    <object-name>cabledirD</object-name>
    <object-name>cabledirG1</object-name>
    <object-name>cabledirD1</object-name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
  </animation>

  <!-- Bloques de freins parking -->
  <animation>
    <type>select</type>
    <object-name>freins</object-name>
    <condition>
      <equals>
        <property>controls/gear/brake-parking</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <!-- L'hélice --> 
  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdisc</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>950</value>
      </greater-than>
    </condition>7
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>300.000</value>
      </greater-than>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>950.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>helice</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
	<value>950</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>moteur</object-name>
    <object-name>helice</object-name>
    <object-name>bol</object-name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -0.529 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.176 </z-m>
    </center>
    <axis>
      <x> -1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>directionG</object-name>
    <object-name>directionG2</object-name>
    <object-name>cabledirG</object-name>
    <property>controls/flight/rudder</property>
    <factor>20</factor>
    <center>
      <x-m>  5.136 </x-m>
      <y-m> -1.923 </y-m>
      <z-m>  0.196 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 0.000 </y>
      <z> 1.000 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>cabledirG1</object-name>
    <property>controls/flight/rudder</property>
    <factor>0.15</factor>
    <center>
      <x-m>  3.680 </x-m>
      <y-m> -2.200 </y-m>
      <z-m> -0.154 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
