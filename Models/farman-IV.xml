    <type>spin</type>
    <object-name>roueG</object-name>
    <property>gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -2.152 </x-m>
      <y-m> -2.145 </y-m>
      <z-m> -1.569 </z-m>
    </center>
